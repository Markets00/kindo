   0101 DD 2A F2 00   [20]  258 	ld 	ix, (ent2_ptr)		;; IX <= ent 2
   0105 DD 96 00      [19]  259 	sub 	Ent_x_I(ix)		;; A <= A - ent2_x
   0108 F2 0D 01      [10]  260 	jp 	p, collision_XR		;; A > 0? lo contrario a A <= 0
