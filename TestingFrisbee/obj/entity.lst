   00F2 00 00               247 ent2_ptr: .dw #0000
   00F4                     248 entityCheckCollision::
                            249 	;;
                            250 	;; If (ent1_x + ent1_w <= ent2_x) no collision
                            251 	;; ent1_x + ent1_w - ent2_x <= 0  no collision
